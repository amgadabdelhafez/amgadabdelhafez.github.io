<?xml version='1.0' encoding='utf-8'?>
<addons><addon id="plugin.video.skipintro" name="Skip Intro Addon" version="1.2.21" provider-name="Amgad Abdelhafez">
    <requires>
        <import addon="xbmc.python" version="3.0.0" />
    </requires>

    <extension point="xbmc.service" library="default.py">
        <provides>video</provides>
    </extension>
    
    <extension point="xbmc.addon.repository" name="Example Repository">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/amgadabdelhafez/amgadabdelhafez.github.io/master/repo/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/amgadabdelhafez/amgadabdelhafez.github.io/master/repo/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/amgadabdelhafez/amgadabdelhafez.github.io/master/repo/zips/</datadir>
        </dir>
    </extension>

    <extension point="xbmc.addon.metadata">
        <summary lang="en">Skip intros/credits in tv shows.</summary>
        <description lang="en">This add-on skips intros during playback.</description>
        <disclaimer />
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon></addons>